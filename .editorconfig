root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 4

[*.cs]
dotnet_diagnostic.warning_level = 9999
treat_warnings_as_errors = true
csharp_style_var_for_built_in_types = true:suggestion
csharp_style_namespace_declarations = file_scoped:warning
